    </div>  

    <div class="col-xl-12">  
        <div class="dashboard__form__wraper">  
            <label>{{ form_label(form.Categorie) }}</label>  
            {{ form_widget(form.Categorie, {'attr': {'class': form_errors(form.Categorie) ? 'form-control is-invalid' : 'form-control'}}) }}  
            <div class="error-message">  
                {{ form_errors(form.Categorie) }}  
            </div>  
        </div>  
    </div>  

<div class="error-message">  
    {{ form_errors(form) }} {# Affiche toutes les erreurs du formulaire #}  
</div>
{% if form.vars.errors is not empty %}  
    <div class="alert alert-danger">  
        <ul>  
            {% for error in form.vars.errors %}  
                <li>{{ error.message }}</li>  
            {% endfor %}  
        </ul>  
    </div>  
{% endif %}
    <div class="col-xl-12">  
        <div class="dashboard__form__button">  
            <button type="submit" class="btn btn-primary">{{ button_label|default('Save') }}</button>  
        </div>  
    </div>  
</div>  

{{ form_end(form) }}
                        </div>  
                    </div>  
        </div>  
    </div>  
</div> 
