<form method="POST" action="{{ path('app_feedback_delete', {'id': feedback.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">  
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ feedback.id) }}">  
    <button class="btn">Supprimer</button>  
</form>