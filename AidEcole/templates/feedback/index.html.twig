{% extends 'base.html.twig' %}

{% block title %}Feedback index{% endblock %}

{% block body %}
    <h1>Feedback index</h1>

    <table class="table">
        <thead>
            <tr>
        
                <th>Nom</th>
                <th>Prenom</th>
                <th>Avis1</th>
                <th>Avis2</th>
                <th>Avis3</th>
                <th>Avis4</th>
                <th>Avis5</th>
                <th>Note</th>

            </tr>
        </thead>
        <tbody>
        {% for feedback in feedback %}
            <tr>
                <td>{{ feedback.Nom }}</td>
                <td>{{ feedback.Prenom }}</td>
                <td>{{ feedback.Avis1}}</td>
                <td>{{ feedback.Avis2}}</td>
                <td>{{ feedback.Avis3}}</td>
                <td>{{ feedback.Avis4}}</td>
                <td>{{ feedback.Avis5}}</td>
                <td>{{ feedback.Note }}</td>
                <td>
                    <a href="{{ path('app_feedback_show', {'id': feedback.id}) }}">show</a>
                    <a href="{{ path('app_feedback_edit', {'id': feedback.id}) }}">edit</a>
                    
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_feedback_new') }}">Create new</a>
{% endblock %}
